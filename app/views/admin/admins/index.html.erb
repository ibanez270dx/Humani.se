<h1>Admins</h1>
<%= link_to 'New Admin', new_admin_admin_path, class: 'btn' %>

<table class="table table-striped">
  <thead>
    <tr><th colspan="2">Login</th></tr>
  </thead>
  <tbody>  
    <% @admins.each do |admin| %>
      <tr>
        <td><%= link_to admin.name, edit_admin_admin_path(admin) %></td>
        <td><%= button_to 'Delete', admin_admin_path(admin), method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %>
      </tr>
    <% end %>
  </tbody>
</table>